<AnchorPane prefHeight="400.0" prefWidth="133.0" stylesheets="@FitspoApp.css" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tdt4140.gr1806.app.ui.MenuController">
